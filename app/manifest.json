{
  "name": "Github Diff Enhancer",
  "version": "0.0.1",
  "description": "Enhance diff pages on Github",

  "manifest_version": 2,

  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },

  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["<all_urls>"],
      "js": [
        "js/content_scripts.js"
      ]
    }
  ]
}
