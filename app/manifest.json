{
  "name": "Github PR Linker",
  "version": "0.0.1",
  "description": "Attach links at PR pages",

  "manifest_version": 2,

  "icons": {
  },

  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["<all_urls>"],
      "js": [
        "js/content_scripts.js"
      ]
    }
  ],

  "permissions": [
    "storage"
  ]
}
